{
    "name": "loyaltycorp/easy-pagination",
    "description": "Provides a generic way to handle pagination data from clients",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^7.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5",
        "laravel/lumen-framework": "^5.5",
        "vlucas/phpdotenv": "^3.3"
    },
    "autoload": {
        "psr-4": {
            "LoyaltyCorp\\EasyPagination\\": "src",
            "LoyaltyCorp\\EasyPagination\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LoyaltyCorp\\EasyPagination\\Tests\\": "tests",
            "LoyaltyCorp\\EasyPagination\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.6-dev"
        }
    }
}
