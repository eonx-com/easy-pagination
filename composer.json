{
    "name": "eonx-com/easy-pagination",
    "description": "Provides a generic way to handle pagination data from clients",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^7.1"
    },
    "require-dev": {
        "eonx-com/easy-psr7-factory": "^2.3.2",
        "phpunit/phpunit": "^8.4",
        "laravel/lumen-framework": "^5.5",
        "vlucas/phpdotenv": "^3.3"
    },
    "autoload": {
        "psr-4": {
            "EonX\\EasyPagination\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "EonX\\EasyPagination\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.4-dev"
        }
    }
}
